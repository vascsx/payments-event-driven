    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>127baea0-b539-489d-8739-e913288ed66a</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>..\..</DockerfileContext>
